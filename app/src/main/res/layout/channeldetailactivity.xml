<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
<include layout="@layout/title_main"></include>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="20dp"
                    android:layout_alignParentRight="true">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="NO:"
                        android:layout_marginRight="5dp"
                        />
                    <TextView
                        android:id="@+id/channelnum_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1235"
                        />

                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="渠道/用户名称:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/channelname_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="渠道类型:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/channelType_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地区:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/district_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="渠道级别:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/channellevel_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系人:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/thecontact_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系电话:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/thecontactphone_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电子邮件:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/email_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="传真:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/faxno_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="税号:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/ein_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:text="开票类型:"
                    />
                <TextView
                    android:id="@+id/makeinvoicetype_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="开户行:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/openaccountbank_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="银行账号:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/bankaccount_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="信誉度:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/credibility_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="法人代表:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/legalrepresentative_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="单位地址:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/unitaddress_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="注册地址:"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:id="@+id/registeredaddress_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1sp"
                android:background="@color/black"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/gray4"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">
                <TextView
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="产品收入信息"
                    android:layout_marginLeft="15dp"
                    android:gravity="center_vertical"
                    android:textSize="20dp"
                    android:textColor="@color/black"/>
            </LinearLayout>
            <TextView
                android:id="@+id/have_not_data_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:text="暂无数据"
                android:visibility="gone"
                />
            <copyright.hxqh.com.copyright.copright.view.MyListView
                android:id="@+id/list"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="40dp"
                android:scrollbars="none"
                android:cacheColorHint="@android:color/transparent"
                android:fadingEdge="none"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </copyright.hxqh.com.copyright.copright.view.MyListView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
